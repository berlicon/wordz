			<Property Name="BodyScript">
				<Value><![CDATA[
	SELECT @exercise_id 'id'
		, e.[name]
		, e.[description]
		, e.[module_id]
		, e.[text]
		, e.[picture_id]
		, e.[ordinal_number]
