		and 
		(
			dbo.fn_is_show_unapproved_exercise(@account_id, 2, e.id) = 0
			and e.parent_id is null
			or
			(
				dbo.fn_is_show_unapproved_exercise(@account_id, 2, e.id) = 1
				and not exists (
						select innerEx.id
						from dbo.exercise_select innerEx
						where innerEx.parent_id = e.id
					)
			)
		)]]></Value>
